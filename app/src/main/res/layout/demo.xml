<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/zddy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <View
            android:layout_width="match_parent"
            android:layout_height="21px"
            android:background="#f5f5f5"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="30px"
            android:paddingRight="30px">

            <View
                android:layout_width="10px"
                android:layout_height="38px"
                android:layout_gravity="center_vertical"
                android:background="#00b660" />

            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="22px"
                android:paddingTop="31px"
                android:paddingBottom="31px"
                android:text="@string/houses_tv45"
                android:id="@+id/zddy_title"
                android:textColor="#000"
                android:layout_gravity="center_vertical"
                android:textSize="32px" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f5f5f5"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/zddy_child1"
            android:paddingLeft="30px"
            android:paddingRight="30px">

            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="31px"
                android:paddingBottom="31px"
                android:text="@string/houses_tv46"
                android:id="@+id/zddy_child1_tv"
                android:textColor="#6e6e6e"
                android:textSize="28px" />

            <TextView android:enabled="false"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15px"
                android:layout_weight="1"
                android:gravity="center_vertical|right"
                android:text="@string/qxz"
                android:id="@+id/qzrq"
                android:textColor="#6e6e6e"
                android:textSize="28px" />

            <ImageView
                android:layout_width="18px"

                android:layout_height="12px"
                android:background="@mipmap/sq1" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:id="@+id/zddy_child2_line"
            android:background="#f5f5f5"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingLeft="30px"
            android:id="@+id/zddy_child2"
            android:paddingRight="30px">

            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="31px"
                android:paddingBottom="31px"
                android:text="@string/houses_tv47"
                android:id="@+id/zddy_child2_tv"
                android:textColor="#6e6e6e"
                android:textSize="28px" />

            <TextView android:enabled="false"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15px"
                android:layout_weight="1"
                android:gravity="center_vertical|right"
                android:text="@string/qxz"
                android:id="@+id/sc"
                android:textColor="#6e6e6e"
                android:textSize="28px" />

            <ImageView
                android:layout_width="18px"
                android:layout_height="12px"

                android:background="@mipmap/sq1" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f5f5f5"
            android:id="@+id/zddy_child3_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingLeft="30px"
            android:id="@+id/zddy_child3"
            android:paddingRight="30px">

            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="31px"
                android:paddingBottom="31px"
                android:text="@string/houses_tv48"
                android:id="@+id/zddy_child3_tv"
                android:textColor="#6e6e6e"
                android:textSize="28px" />

            <TextView android:enabled="false"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15px"
                android:layout_weight="1"
                android:gravity="center_vertical|right"
                android:text="@string/qxz"
                android:id="@+id/zfjd"
                android:textColor="#6e6e6e"
                android:textSize="28px" />

            <ImageView
                android:layout_width="18px"
                android:layout_height="12px"

                android:background="@mipmap/sq1" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f5f5f5"
            android:id="@+id/zddy_child4_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingLeft="30px"
            android:id="@+id/zddy_child4"
            android:paddingRight="30px">

            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="31px"
                android:paddingBottom="31px"
                android:text="@string/houses_tv49"
                android:id="@+id/zddy_child4_tv"
                android:textColor="#6e6e6e"
                android:textSize="28px" />
            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="7px"
                android:layout_marginRight="28px"
                android:gravity="center"
                android:text="*"
                android:id="@+id/zddy_child4_tv1"
                android:visibility="gone"
                android:textColor="#ff3366"
                android:textSize="32px" />

            <TextView android:enabled="false"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@null"
                android:hint="@string/qsr"
                android:gravity="center_vertical|right"
                android:textColor="#6e6e6e"
                android:textSize="28px"
                android:textColorHint="#9b9b9b"
                android:paddingLeft="30px"
                android:paddingRight="0px"
                android:inputType="number"
                android:id="@+id/qwzj"
                android:textCursorDrawable="@drawable/edit_cursor"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f5f5f5"
            android:id="@+id/zddy_child5_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingLeft="30px"
            android:id="@+id/zddy_child5"
            android:paddingRight="30px"
            android:visibility="gone">

            <TextView android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="31px"
                android:paddingBottom="31px"
                android:text="@string/houses_tv50"
                android:id="@+id/zddy_child5_tv"
                android:textColor="#6e6e6e"
                android:textSize="28px" />
            <TextView android:enabled="false"
                android:id="@+id/zddy_child5_tv1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="7px"
                android:layout_marginRight="28px"
                android:gravity="center"
                android:text="*"
                android:visibility="gone"
                android:textColor="#ff3366"
                android:textSize="32px" />

            <TextView android:enabled="false"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@null"
                android:hint="@string/qsr"
                android:id="@+id/zdzj"
                android:inputType="number"
                android:gravity="center_vertical|right"
                android:textColor="#6e6e6e"
                android:textSize="28px"
                android:textColorHint="#9b9b9b"
                android:paddingLeft="30px"
                android:paddingRight="0px"
                android:textCursorDrawable="@drawable/edit_cursor"
                />
        </LinearLayout>

    </LinearLayout>
</LinearLayout>